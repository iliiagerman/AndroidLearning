        <activity android:name=".Lesons20_40.ActivityResult.ActivityResultColor" />
        <activity android:name=".Lesons20_40.ActivityResult.ResultActivity" />
        <activity android:name=".Lesons20_40.SQLifeActivity" />
        <activity android:name=".Lesons20_40.PreferencesActivity" />
